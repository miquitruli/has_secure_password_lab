<h1>Users#new</h1>
<p>Find me in app/views/users/new.html.erb</p>

<%= form_for :user do |f| %>
    <p>Username: <%= f.text_field :name %></p>
    <p>Password: <%= f.password_field :password %></p>
    <p>Password Confirmation: <%= f.password_field :password_confirmation %></p>
    <%= f.submit 'Submit' %>
<% end %> 
